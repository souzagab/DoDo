<header class="bg-white">
  <nav class="mx-auto flex max-w-7xl items-center justify-between p-6 lg:px-8" aria-label="Global">
    <div class="flex flex-1">
    </div>
    <a href="#" class="-m-1.5 p-1.5">
      <span class="sr-only">DoDo</span>
      <img class="h-8 w-auto" src="https://tailwindui.com/img/logos/mark.svg?color=indigo&shade=600" alt="">
    </a>
    <div class="flex flex-1 justify-end">
     <% if user_signed_in? %>
        <%= button_to raw('Log out <span aria-hidden="true">&rarr;</span></a>'), session_path(current_session), method: :delete, class: "text-sm font-semibold leading-6 text-gray-900" %>
      <% else %>
        <%= link_to raw('Log in <span aria-hidden="true">&rarr;</span></a>'), sign_in_path, class: "text-sm font-semibold leading-6 text-gray-900" %>
    <% end %>
    </div>
  </nav>
</header>